<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="Slickflow Admin">

    <link rel="shortcut icon" href="/MvcDemo/Scripts/flatlab/img/favicon.png">

    <title>Slickflow 企业级工作流引擎解决方案</title>

    <!-- Bootstrap core CSS -->
    <link href="/MvcDemo/Scripts/flatlab/css/bootstrap.min.css" rel="stylesheet">
    <link href="/MvcDemo/Scripts/flatlab/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="/MvcDemo/Scripts/flatlab/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="/MvcDemo/Scripts/flatlab/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="stylesheet" href="/MvcDemo/Scripts/flatlab/css/owl.carousel.css" type="text/css">

    <!--right slidebar-->
    <link href="/MvcDemo/Scripts/flatlab/css/slidebars.css" rel="stylesheet">

    <!-- Custom styles for this template -->

    <link href="/MvcDemo/Scripts/flatlab/css/style.css" rel="stylesheet">
    <link href="/MvcDemo/Scripts/flatlab/css/style-responsive.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <section id="container">
        <!--header start-->
        <header class="header white-bg">
            <!--logo start-->
            <a href="/MvcDemo/Home/Index" class="logo">Slick<span>flow</span></a>
            <!--logo end-->

            <div class="top-nav ">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder="Search">
                    </li>
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="/MvcDemo/Scripts/flatlab/img/avatar1_small.jpg">
                            <span class="username" id="spnLogonUser">Unknown User</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <li><a href="/MvcDemo/Account/Login" id="btnLogount" onclick="onUserLogout();"><i class="fa fa-key"></i> 退 出</a></li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!--search & user info end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">
                    <li class="sub-menu">
                        <a href="javascript:;" class="active">
                            <i class="fa fa-tasks"></i>
                            <span>订单流程演示</span>
                        </a>
                        <ul class="sub">
                            <li><a href="/MvcDemo/POrder/Index">订单数据(Orders)</a></li>
                            <li><a href="/MvcDemo/KGraph/Index">流程图示(FlowChart)</a></li>
                            @*<li><a href="../Hrs/Index">员工请假流程</a></li>*@
                        </ul>
                    </li>
                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
    </section>
    <script src="/MvcDemo/Scripts/flatlab/js/jquery.js"></script>

    @RenderBody()

    <!-- js placed at the end of the document so the pages load faster -->
    <script src="/MvcDemo/Scripts/flatlab/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="/MvcDemo/Scripts/flatlab/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/MvcDemo/Scripts/flatlab/js/jquery.scrollTo.min.js"></script>
    <script src="/MvcDemo/Scripts/flatlab/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="/MvcDemo/Scripts/flatlab/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="/MvcDemo/Scripts/flatlab/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="/MvcDemo/Scripts/flatlab/js/owl.carousel.js"></script>
    <script src="/MvcDemo/Scripts/flatlab/js/jquery.customSelect.min.js"></script>
    <script src="/MvcDemo/Scripts/flatlab/js/respond.min.js"></script>

    <!--right slidebar-->
    <script src="/MvcDemo/Scripts/flatlab/js/slidebars.min.js"></script>

    <!--common script for all pages-->
    <script src="/MvcDemo/Scripts/flatlab/js/common-scripts.js"></script>

    <!--script for this page-->
    <script src="/MvcDemo/Scripts/flatlab/js/sparkline-chart.js"></script>
    <script src="/MvcDemo/Scripts/flatlab/js/easy-pie-chart.js"></script>
    <script src="/MvcDemo/Scripts/flatlab/js/count.js"></script>

    <!-- script helper-->
    <script src="/MvcDemo/Scripts/jshelper.js"></script>
    <script src="/MvcDemo/Scripts/localhelper.js"></script>
    <script src="/MvcDemo/AppJS/define.js"></script>

    <!-- script for application module -->
    <script type="text/javascript">
        $(function (e) {
            var user = lsm.getUserIdentity();

            if (user != null) {
                $("#spnLogonUser").text(user.UserName);
            }
            else {
                window.location.replace("/MvcDemo/Account/Login");
            }
        })

        function onUserLogout() {
            lsm.removeTempStorage();
        }
    </script>
</body>
</html>
