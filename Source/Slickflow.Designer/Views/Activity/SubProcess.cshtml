@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>子流程节点编辑</title>
</head>
<body>
    <!-- Modal -->
    <div>
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="myModalLabel">子流程节点编辑</h3>
        </div>
        <div class="modal-body">
            <div>
                <p>
                    <span class="a_tit">子流程GUID:</span>
                    <span><input type="text" id="txtProcessGUID" style="width:340px;" readonly/></span>
                    <span class="a_tit">子流程名称:</span>
                    <span><input type="text" id="txtProcessName" style="width:340px;" readonly/></span>
                </p>
            </div>
            <div id="mySubProcessGrid" class="grid-container" style="width:100%;height:260px;"></div>
        </div>
        <div class="modal-footer">
            <button class="btn btn-primary" onclick="subprocessmanager.saveSubProcess();">确定</button>
            <button class="btn" data-dismiss="modal" aria-hidden="true">取消</button>
        </div>
    </div>

    <script type="text/javascript">
        $(function () {
            subprocessmanager.load();
        })
    </script>
</body>
</html>