
@{
    Layout = null;
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>流程数据</title>
</head>
<body>
    <!-- Modal -->
    <div>
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h3 id="myModalLabel">流程数据</h3>
        </div>
        <div class="modal-body">
            <img src="~/Content/images/ajax-loader.gif" id="loading-indicator" style="position: fixed; left: 50%; top: 40%; display: none;" />
            <div>
                <b class="icoBtn btnNew" onclick="processlist.createProcess();"></b>
                <b class="icoBtn btnEdit" onclick="processlist.editProcess();"></b>
            </div>
            <div id="myProcessGrid" class="grid-container" style="width:100%;height:300px;float:left;"></div>
        </div>
        <div class="modal-footer">
            <button class="btn btn-primary" onclick="processlist.sure();">确定</button>
            <button class="btn" data-dismiss="modal" aria-hidden="true">取消</button>
            <button class="btn btn-danger" onclick="processlist.deleteProcess();">删除</button>
        </div>
    </div>

    <script type="text/javascript">
        $(function () {
            processlist.initListForm();

            processlist.getProcessList();
        });
    </script>
</body>
</html>